Download from http://packages.bic.mni.mcgill.ca/tgz/mni_cortical_statistics-0.9.4.tar.gz
tar -xzf mni_cortical_statistics-0.9.4.tar.gz
echo 'exportPattern( "." )' > mni_cortical_statistics-0.9.4/NAMESPACE
