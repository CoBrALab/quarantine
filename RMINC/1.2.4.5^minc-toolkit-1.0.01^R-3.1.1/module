#%Module1.0#####################################################################

module-whatis   "adds RMINC 1.2.4.5 to your environment"

set quarantine_path  $::env(QUARANTINE_PATH)

prereq minc-toolkit/1.0.01
prereq R/3.1.1
prereq minc-stuffs

set basepath  ${quarantine_path}/RMINC/1.2.4.5^minc-toolkit-1.0.01^R-3.1.1/build

prepend-path  R_LIBS_USER   ${basepath}
