#%Module1.0#####################################################################

module-whatis   "adds RMINC 1.2.4.5 to your environment"

set quarantine_path  $::env(QUARANTINE_PATH)

prereq minc-toolkit/1.9.10
prereq R/3.2.1 MRO
prereq minc-stuffs

set basepath  ${quarantine_path}/RMINC/1.2.4.9^minc-toolkit-1.9.10^R-3.2.1/build

prepend-path  R_LIBS   ${basepath}
