#%Module1.0#####################################################################

module-whatis   "adds Rstudio to your environment"

prereq MRO R
conflict rstudio

set quarantine_path  $::env(QUARANTINE_PATH)

set basepath     ${quarantine_path}/rstudio/0.99.491/build/rstudio-0.99.491

prepend-path     PATH                       ${basepath}/bin
